{
  "icon_theme": "Material Icon Theme",
  "agent": {
    "default_profile": "ask",
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-7-sonnet-thinking-latest"
    },
    "model_parameters": [],
    "always_allow_tool_actions": true,
    "version": "2"
  },
  "relative_line_numbers": true,
  "theme": "Catppuccin Mocha",
  "buffer_font_size": 13,
  "format_on_save": "on",
  "formatter": "language_server",
  "tab_size": 2,
  "buffer_font_family": "Hack",
  "confirm_quit": true,
  "show_whitespaces": "all",
  "terminal": {
    "font_family": "Hack",
    "font_size": 13
  },
  "languages": {
    "TypeScript": {
      "language_servers": [
        "typescript-language-server"
      ]
    }
  },
  "vim_mode": true
}
